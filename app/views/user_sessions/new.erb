<%= form_with url: login_path do |f| %>
    <div class="mb-3">
        <%= f.label :email ,t('users.email') %>
        <%= f.email_field :email %>
    </div>
    <div class="mb-3">
        <%= f.label :password,t('users.password') %>
        <%= f.password_field :password %>
    </div>   
    <%= f.submit "ログイン" %>
    
<% end %>
  